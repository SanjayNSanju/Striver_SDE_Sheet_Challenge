#include <bits/stdc++.h>

int subarraysXor(vector<int> &arr, int x)
{
  int n = arr.size();
   int t=0,ans=0;
   map<int,int> mp;
   mp[t]++;
   for(int i=0;i<n;i++)
   {
      t = t^arr[i];
      if(mp.find(t^x)!=mp.end())
      {
        ans = ans+mp[t^x];
      }
      mp[t]++;
   }
   return ans;
}


